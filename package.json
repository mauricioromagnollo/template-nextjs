{
  "name": "template-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"{src,test}/**/*.{ts,js,tsx,jsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx vitest --no-cache --passWithNoTests",
    "test:watch": "npm t -- -c vitest.config.unit.mts watch",
    "test:unit": "npm t -- -c vitest.config.unit.mts run",
    "test:staged": "npm t -- -c vitest.config.mts run related --silent",
    "test:coverage": "npm t -- -c vitest.config.mts --fileParallelism=false run --coverage"
  },
  "dependencies": {
    "next": "14.2.12",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.1.1",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.12",
    "jsdom": "25.0.0",
    "nock": "13.5.5",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "0.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  }
}
